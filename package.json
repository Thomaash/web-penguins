{
  "name": "web-penguins",
  "version": "0.1.0",
  "license": "ISC",
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:js": "webpack --config webpack.prod.js -p",
    "build:types": "tsc --emitDeclarationOnly",
    "fix": "eslint --fix --ext .js,.ts .",
    "lint": "eslint --ext .js,.ts .",
    "serve": "webpack-dev-server --progress --profile --color --watch --mode development",
    "serve:external": "webpack-dev-server --host 0.0.0.0 --progress --profile --color --watch --mode development",
    "ts:build": "npm run build:types && npm run build:js",
    "ts:build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "ts:build:types": "tsc --emitDeclarationOnly",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-typescript": "^7.3.3",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "eslint": "^5.16.0",
    "eslint-config-standard-with-typescript": "^7.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.3.3",
    "url-loader": "^1.1.2",
    "web-animations-js": "^2.3.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "line-intersect": "^2.2.0"
  }
}
